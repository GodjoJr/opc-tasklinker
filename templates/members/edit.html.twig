{% extends 'base.html.twig' %}

{% block custom_title %}
<h1>
	Modifier un membre
</h1>
{% endblock %}

{% block body %}

	<div id="content">
		{{ form_start(form) }}
			<div>{{ form_row(form.lastname) }}</div>
			<div>{{ form_row(form.firstname) }}</div>
			<div>{{ form_row(form.email) }}</div>
				
			<div class="form-flex">
				<div>{{ form_row(form.entry_date) }}</div>
				<div>{{ form_row(form.status) }}</div>
			</div>

			<button class="button button-submit">Enregistrer</button>
		{{ form_end(form) }}
	</div>
{% endblock %}
